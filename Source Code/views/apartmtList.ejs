    <main>
      <div class="box">
        <br />
        <form class="d-flex" id="searchApartmtList">
          <input
            class="form-control me-2 searchbtn"
            type="search"
            id="inputFieldAptmt"
            placeholder="Search apartments"
            aria-label="Search"
          />
          <button
            class="btn btn-outline-success"
            id="searchApmnt"
            type="submit"
          >
            Search
          </button>
        </form>
        <button class="btn btn-outline-success" id="addAptmt" type="submit">
          Add
        </button>

        <form class="filters">
          <input type="checkbox" id="price" />
          <label class="label-inline" for="price">Price</label>
          <br /><br />
          <input type="checkbox" id="bedrooms" />
          <label class="label-inline" for="bedrooms">Bedrooms</label>
          <br /><br />
          <input type="checkbox" id="pet" />
          <label class="label-inline" for="pet">PetFriendly</label>
          <br /><br />
          <input type="checkbox" id="utilities" />
          <label class="label-inline" for="utilities">Utilities Included</label>
          <br /><br />
          <input type="checkbox" id="noUtilities" />
          <label class="label-inline" for="noUtilities">
            No Utilities Included</label
          >
          <br /><br />
          <input type="checkbox" id="oneYear" />
          <label class="label-inline" for="oneYear">1 Year Lease</label>
          <br /><br />
          <input type="checkbox" id="sixMonths" />
          <label class="label-inline" for="sixMonths">6 months lease</label>
        </form>
        <form>
          <!--<img id="circle1" src="public/images/boardwalk.jpg" />
          
          <img id="circle2" src="public/images/quappelle.jpg" />

          <img id="circle3" src="public/images/sterlingManor.jpg" />-->

